  number = {6}
}

@article{sapolskyHowGlucocorticoidsInfluence2000,
  ids = {sapolsky_how_2000},
  title = {How Do Glucocorticoids Influence Stress Responses? {{Integrating}} Permissive, Suppressive, Stimulatory, and Preparative Actions},
  shorttitle = {How Do Glucocorticoids Influence Stress Responses?},
  author = {Sapolsky, R. M. and Romero, L. M. and Munck, A. U.},
  date = {2000-02},
  journaltitle = {Endocrine Reviews},
  shortjournal = {Endocr. Rev.},
  volume = {21},
  pages = {55--89},
  issn = {0163-769X},
  doi = {10.1210/edrv.21.1.0389},
  abstract = {The secretion of glucocorticoids (GCs) is a classic endocrine response to stress. Despite that, it remains controversial as to what purpose GCs serve at such times. One view, stretching back to the time of Hans Selye, posits that GCs help mediate the ongoing or pending stress response, either via basal levels of GCs permitting other facets of the stress response to emerge efficaciously, and/or by stress levels of GCs actively stimulating the stress response. In contrast, a revisionist viewpoint posits that GCs suppress the stress response, preventing it from being pathologically overactivated. In this review, we consider recent findings regarding GC action and, based on them, generate criteria for determining whether a particular GC action permits, stimulates, or suppresses an ongoing stress-response or, as an additional category, is preparative for a subsequent stressor. We apply these GC actions to the realms of cardiovascular function, fluid volume and hemorrhage, immunity and inflammation, metabolism, neurobiology, and reproductive physiology. We find that GC actions fall into markedly different categories, depending on the physiological endpoint in question, with evidence for mediating effects in some cases, and suppressive or preparative in others. We then attempt to assimilate these heterogeneous GC actions into a physiological whole.},
  eprint = {10696570},
  eprinttype = {pmid},
  file = {C\:\\Users\\Megaport\\Zotero\\storage\\2CK3EZTL\\Sapolsky et al. - 2000 - How Do Glucocorticoids Influence Stress Responses.pdf;C\:\\Users\\Megaport\\Zotero\\storage\\FJVT497C\\Sapolsky et al. - 2000 - How Do Glucocorticoids Influence Stress Responses.pdf;C\:\\Users\\Megaport\\Zotero\\storage\\2QH26ZGS\\2423840.html;C\:\\Users\\Megaport\\Zotero\\storage\\TJ47JHT2\\2423840.html},
  keywords = {Animals,Body Fluids,Cardiovascular System,Endocrinology,Glucocorticoids,Humans,Inflammation,Nervous System,Physiological,Reproduction,Stress,Stress; Physiological},
  langid = {english},
  number = {1}
}

@article{sapolskyInfluenceSocialHierarchy2005,
  ids = {sapolsky_influence_2005},
  title = {The {{Influence}} of {{Social Hierarchy}} on {{Primate Health}}},
  author = {Sapolsky, Robert M.},
  date = {2005-04-29},
  journaltitle = {Science},
  volume = {308},
  pages = {648--652},
  issn = {0036-8075, 1095-9203},
  doi = {10.1126/science.1106477},
  url = {https://science.sciencemag.org/content/308/5722/648},
  urldate = {2019-05-03},
  abstract = {Dominance hierarchies occur in numerous social species, and rank within them can greatly influence the quality of life of an animal. In this review, I consider how rank can also influence physiology and health. I first consider whether it is high- or low-ranking animals that are most stressed in a dominance hierarchy; this turns out to vary as a function of the social organization in different species and populations. I then review how the stressful characteristics of social rank have adverse adrenocortical, cardiovascular, reproductive, immunological, and neurobiological consequences. Finally, I consider how these findings apply to the human realm of health, disease, and socioeconomic status.},
  copyright = {American Association for the Advancement of Science},
