@article{cookConditionDependentIntraindividual2012,
  ids = {cook_condition_2012},
  title = {Condition Dependent Intra-Individual Repeatability of Stress-Induced Cortisol in a Freshwater Fish},
  author = {Cook, K. V. and O'Connor, C. M. and McConnachie, S. H. and Gilmour, K. M. and Cooke, S. J.},
  date = {2012-03},
  journaltitle = {Comparative Biochemistry and Physiology. Part A, Molecular \& Integrative Physiology},
  shortjournal = {Comp. Biochem. Physiol., Part A Mol. Integr. Physiol.},
  volume = {161},
  pages = {337--343},
  issn = {1531-4332},
  doi = {10.1016/j.cbpa.2011.12.002},
  abstract = {The glucocorticoid (GC) stress response is thought to be an individual trait associated with behaviour and life history strategies. Studies exploring such relationships typically assume measured hormone values to be repeatable within an individual. However, repeatability of GCs has proven variable in wild animals and underlying reasons remain unknown. We assessed individual repeatability of circulating stress-induced cortisol, the primary GC in teleost fish, and glucose concentrations in a wild teleost fish held under consistent laboratory conditions. We also tested the hypothesis that the magnitude of intra-individual variability in stress-induced cortisol concentrations ("cortisol variability") is influenced by body condition. Wild-caught bluegill sunfish (Lepomis macrochirus) were subjected to repeated standardized stressors and blood sampled (3 times over 6 days) once cortisol concentrations peaked. Various indicators of fish condition, both whole body and physiological, were also measured. Overall, stress-induced circulating cortisol concentrations were repeatable but stress-induced glucose was not. Cortisol variability was related to Fulton's condition factor and size (eviscerated mass) where smaller fish in poor condition exhibited increased cortisol variability. The findings have implications for the interpretation of studies that examine correlates of GC concentrations as they suggest consistency in stress responsiveness is influenced by factors such as size and condition.},
  eprint = {22179071},
  eprinttype = {pmid},
  file = {C\:\\Users\\Megaport\\Zotero\\storage\\NZZYYIVE\\Cook et al. - 2012 - Condition dependent intra-individual repeatability.pdf;C\:\\Users\\Megaport\\Zotero\\storage\\3WT2MIXN\\S109564331100331X.html},
  keywords = {Animals,Blood Glucose,Blood Proteins,Body Weight,Condition factor,Cortisol,Female,Fish,Glucocorticoid secretion,Hematocrit,Hydrocortisone,Individual variation,Male,Perciformes,Physiological,Repeatability,Reproducibility of Results,Sex Factors,Stress,Stress response,Stress; Physiological},
  langid = {english},
  number = {3}
}

@article{copelandPubertyChimpanzeeSomatomedinC1985,
  ids = {copeland_puberty_1985},
  title = {Puberty in the Chimpanzee: Somatomedin-{{C}} and Its Relationship to Somatic Growth and Steroid Hormone Concentrations},
  shorttitle = {Puberty in the Chimpanzee},
  author = {Copeland, K. C. and Eichberg, J. W. and Parker, C. R. and Bartke, A.},
