@article{silkFemaleChacmaBaboons2010,
  ids = {silk_female_2010},
  title = {Female Chacma Baboons Form Strong, Equitable, and Enduring Social Bonds},
  author = {Silk, Joan and Beehner, Jacinta C. and Bergman, Thore J. and Crockford, Catherine and Engh, Anne L. and Moscovice, Liza R. and Wittig, Roman M. and Seyfarth, Robert M. and Cheney, Dorothy L.},
