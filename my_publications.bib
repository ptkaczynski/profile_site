  author = {Goodyer, I. M. and Park, R. J. and Netherton, C. M. and Herbert, J.},
  date = {2001-09},
  journaltitle = {The British Journal of Psychiatry},
  volume = {179},
  pages = {243--249},
  issn = {0007-1250, 1472-1465},
  doi = {10.1192/bjp.179.3.243},
  url = {https://www.cambridge.org/core/journals/the-british-journal-of-psychiatry/article/possible-role-of-cortisol-and-dehydroepiandrosterone-in-human-development-and-psychopathology/30027A32CDB1C3AD30D1EAA4FB7E4067},
  urldate = {2019-01-09},
  abstract = {Background The characteristics of adrenal hormone secretion change markedly during infancy. Disturbances in basal levels may precipitate psychological dysfunction and are associated with psychopathology in young people. Aims To relate three aspects of behavioural endocrinology: developmental changes in cortisol and dehydroepiandrosterone (DHEA), the role of these hormones in the psychopathology of young people, and the action of these steroids in the brain. Method A selective review from the human developmental, psychiatric and neurosciences literature. Results There are developmentally mediated changes in brain sensitivity following excess exposure to cortisol. This may result in impairments of mental and behavioural function. DHEA and gonadal steroids may modulate the actions of cortisol. Conclusions Steroid hormones contribute to shaping behavioural function during early development and act as risk factors for psychopathology.},
  file = {C\:\\Users\\Megaport\\Zotero\\storage\\86VDFDHG\\Goodyer et al. - 2001 - Possible role of cortisol and dehydroepiandrostero.pdf;C\:\\Users\\Megaport\\Zotero\\storage\\RBS5SE6W\\Goodyer et al. - 2001 - Possible role of cortisol and dehydroepiandrostero.pdf;C\:\\Users\\Megaport\\Zotero\\storage\\5MIS4Y45\\30027A32CDB1C3AD30D1EAA4FB7E4067.html;C\:\\Users\\Megaport\\Zotero\\storage\\NJ747A7B\\core-reader.html},
  langid = {english},
