  file = {C\:\\Users\\Megaport\\Zotero\\storage\\ABURFBFS\\9781118963418.html;C\:\\Users\\Megaport\\Zotero\\storage\\KD9YE7XP\\9781118963418.html;C\:\\Users\\Megaport\\Zotero\\storage\\XA66ZCGA\\9781118963418.html},
  isbn = {978-1-118-96341-8},
  keywords = {11beta-hydroxysteroid dehydrogenase type 2,assisted reproductive technology,cortisol,development,epigenetics,evolution,fetal growth,life history theory,maternal nutrition,placenta,pregnancy,prenatal environment,prenatal stress,reproductive strategies,toxin and pollutant exposures,transgenerational effects},
