@article{formicaFitnessConsequencesSocial2012,
  title = {Fitness Consequences of Social Network Position in a Wild Population of Forked Fungus Beetles ({{Bolitotherus}} Cornutus)},
  author = {Formica, V. A. and Wood, C. W. and Larsen, W. B. and Butterfield, R. E. and Augat, M. E. and Hougen, H. Y. and Brodie, E. D.},
