@article{inoue-nakamuraDevelopmentStoneTool1997,
  ids = {inoue-nakamura_development_1997},
  title = {Development of Stone Tool Use by Wild Chimpanzees ({{Pan}} Troglodytes)},
  author = {Inoue-Nakamura, N. and Matsuzawa, T.},
