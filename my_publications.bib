}

@article{mitaniMaleChimpanzeesForm2009,
  ids = {mitaniMaleChimpanzeesForm2009a,mitani_male_2009,mitani_male_2009-1},
  title = {Male Chimpanzees Form Enduring and Equitable Social Bonds},
