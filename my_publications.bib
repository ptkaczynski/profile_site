@article{kaplanTheoryHumanLife2000,
  ids = {kaplan_theory_2000},
  title = {A Theory of Human Life History Evolution: {{Diet}}, Intelligence, and Longevity},
  shorttitle = {A Theory of Human Life History Evolution},
  author = {Kaplan, Hillard and Hill, Kim and Lancaster, Jane and Hurtado, A. Magdalena},
  date = {2000},
  journaltitle = {Evolutionary Anthropology: Issues, News, and Reviews},
  volume = {9},
