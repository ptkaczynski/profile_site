@article{biroSmallWithindayIncreases2010a,
  ids = {biro_small_2010},
  title = {Small Within-Day Increases in Temperature Affects Boldness and Alters Personality in Coral Reef Fish},
  author = {Biro, Peter A. and Beckmann, Christa and Stamps, Judy A.},
  date = {2010-01-07},
  journaltitle = {Proceedings of the Royal Society B: Biological Sciences},
  shortjournal = {Proc Biol Sci},
  volume = {277},
  pages = {71--77},
  issn = {0962-8452},
  doi = {10.1098/rspb.2009.1346},
  url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC2842624/},
  urldate = {2019-08-30},
  abstract = {Consistent individual differences in behaviour, termed personality, are common in animal populations and can constrain their responses to ecological and environmental variation, such as temperature. Here, we show for the first time that normal within-daytime fluctuations in temperature of less than 3°C have large effects on personality for two species of juvenile coral reef fish in both observational and manipulative experiments. On average, individual scores on three personality traits (PTs), activity, boldness and aggressiveness, increased from 2.5- to sixfold as a function of temperature. However, whereas most individuals became more active, aggressive and bold across temperature contexts (were plastic), others did not; this changed the individual rank order across temperatures and thus altered personality. In addition, correlations between PTs were consistent across temperature contexts, e.g. fish that were active at a given temperature also tended to be both bold and aggressive. These results (i) highlight the importance of very carefully controlling for temperature when studying behavioural variation among and within individuals and (ii) suggest that individual differences in energy metabolism may contribute to animal personality, given that temperature has large direct effects on metabolic rates in ectotherms.},
  eprint = {19793748},
  eprinttype = {pmid},
  file = {C\:\\Users\\Megaport\\Zotero\\storage\\FKEDYILY\\Biro et al. - 2010 - Small within-day increases in temperature affects .pdf},
  number = {1678},
  pmcid = {PMC2842624}
}

@article{biroUsingRepeatabilityStudy2015a,
  ids = {biro_using_2015},
  title = {Using Repeatability to Study Physiological and Behavioural Traits: Ignore Time-Related Change at Your Peril},
  shorttitle = {Using Repeatability to Study Physiological and Behavioural Traits},
  author = {Biro, Peter A. and Stamps, Judy A.},
  date = {2015-07-01},
  journaltitle = {Animal Behaviour},
  shortjournal = {Animal Behaviour},
  volume = {105},
  pages = {223--230},
  issn = {0003-3472},
  doi = {10.1016/j.anbehav.2015.04.008},
  url = {http://www.sciencedirect.com/science/article/pii/S0003347215001451},
  urldate = {2018-07-04},
  abstract = {Broad sense repeatability, which refers to the extent to which individual differences in trait scores are maintained over time, is of increasing interest to researchers studying behavioural or physiological traits. Broad sense repeatability is most often inferred from the statistic R (the intraclass correlation, or narrow sense repeatability). However, R ignores change over time, despite the inherent longitudinal nature of the data (repeated measures over time). Here, we begin by showing that most studies ignore time-related change when estimating broad sense repeatability, and estimate R with low statistical power. Given this problem, we (1) outline how and why ignoring time-related change in scores (that occurs for whatever reason) can seriously affect estimates of the broad sense repeatability of behavioural or physiological traits, (2) discuss conditions in which various indices of R can or cannot provide reliable estimates of broad sense repeatability, and (3) provide suggestions for experimental designs for future studies. Finally, given that we already have abundant evidence that many labile traits are ‘repeatable’ in that broad sense (i.e. R{$>$}0), we suggest a shift in focus towards obtaining robust estimates of the repeatability of behavioural and physiological traits. Given how labile these traits are, this will require greater experimental (and/or statistical) control and larger sample sizes in order to detect and quantify change over time (if present).},
  file = {C\:\\Users\\Megaport\\Zotero\\storage\\4CE5VSDC\\Biro and Stamps - 2015 - Using repeatability to study physiological and beh.pdf;C\:\\Users\\Megaport\\Zotero\\storage\\CCMFBVVD\\Biro and Stamps - 2015 - Using repeatability to study physiological and beh.pdf;C\:\\Users\\Megaport\\Zotero\\storage\\EPK25IP9\\Biro and Stamps - 2015 - Using repeatability to study physiological and beh.pdf;C\:\\Users\\Megaport\\Zotero\\storage\\YJ5JLLCI\\Biro and Stamps - 2015 - Using repeatability to study physiological and beh.pdf;C\:\\Users\\Megaport\\Zotero\\storage\\76BPZZB5\\S0003347215001451.html;C\:\\Users\\Megaport\\Zotero\\storage\\CBQY64JL\\S0003347215001451.html;C\:\\Users\\Megaport\\Zotero\\storage\\FIRW8JA4\\S0003347215001451.html;C\:\\Users\\Megaport\\Zotero\\storage\\RW5Y5X68\\S0003347215001451.html},
  keywords = {behavioural syndromes,metabolism,mixed models,personality,plasticity}
}

@article{blacklessHowSexuallyDimorphic2000b,
  ids = {blacklessHowSexuallyDimorphic2000a,blackless_how_2000,blackless_how_2000-1},
  title = {How Sexually Dimorphic Are We? {{Review}} and Synthesis},
  shorttitle = {How Sexually Dimorphic Are We?},
  author = {Blackless, Melanie and Charuvastra, Anthony and Derryck, Amanda and Fausto‐Sterling, Anne and Lauzanne, Karl and Lee, Ellen},
  date = {2000},
  journaltitle = {American Journal of Human Biology},
  volume = {12},
  pages = {151--166},
  issn = {1520-6300},
  doi = {10.1002/(SICI)1520-6300(200003/04)12:2<151::AID-AJHB1>3.0.CO;2-F},
  url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/%28SICI%291520-6300%28200003/04%2912%3A2%3C151%3A%3AAID-AJHB1%3E3.0.CO%3B2-F},
  urldate = {2019-02-28},
  abstract = {The belief that Homo sapiens is absolutely dimorphic with the respect to sex chromosome composition, gonadal structure, hormone levels, and the structure of the internal genital duct systems and external genitalia, derives from the platonic ideal that for each sex there is a single, universally correct developmental pathway and outcome. We surveyed the medical literature from 1955 to the present for studies of the frequency of deviation from the ideal male or female. We conclude that this frequency may be as high as 2\% of live births. The frequency of individuals receiving “corrective” genital surgery, however, probably runs between 1 and 2 per 1,000 live births (0.1–0.2\%). Am. J. Hum. Biol. 12:151–166, 2000. © 2000 Wiley-Liss, Inc.},
  copyright = {Copyright © 2000 Wiley‐Liss, Inc.},
  file = {C\:\\Users\\Megaport\\Zotero\\storage\\CI597MVC\\Blackless et al. - 2000 - How sexually dimorphic are we Review and synthesi.pdf;C\:\\Users\\Megaport\\Zotero\\storage\\FMNAVJNK\\Blackless et al. - 2000 - How sexually dimorphic are we Review and synthesi.pdf;C\:\\Users\\Megaport\\Zotero\\storage\\G8BAYALD\\Blackless et al. - 2000 - How sexually dimorphic are we Review and synthesi.pdf;C\:\\Users\\Megaport\\Zotero\\storage\\IWUN453H\\04)122151AID-AJHB13.0.html},
