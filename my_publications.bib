@article{nishidaFoodTransferMother1996,
  ids = {nishida_food_1996},
  title = {Food Transfer between Mother and Infant Chimpanzees of the {{Mahale Mountains National Park}}, {{Tanzania}}},
  author = {Nishida, Toshisada and Turner, Linda A.},
  date = {1996-12-01},
  journaltitle = {International Journal of Primatology},
  shortjournal = {Int J Primatol},
  volume = {17},
