  title = {Wild Bonobo and Chimpanzee Females Exhibit Broadly Similar Patterns of Behavioral Maturation but Some Evidence for Divergence},
  author = {Lee, Sean M. and Murray, Carson M. and Lonsdorf, Elizabeth V. and Fruth, Barbara and Stanton, Margaret A. and Nichols, Jennifer and Hohmann, Gottfried},
