@article{dingemanseQuantifyingIndividualVariation2013,
  ids = {dingemanse_quantifying_2013},
  title = {Quantifying Individual Variation in Behaviour: Mixed-Effect Modelling Approaches},
  shorttitle = {Quantifying Individual Variation in Behaviour},
  author = {Dingemanse, Niels J. and Dochtermann, Ned A.},
  date = {2013},
  journaltitle = {Journal of Animal Ecology},
  volume = {82},
