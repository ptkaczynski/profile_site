@article{fosterSocialNetworkCorrelates2012,
  ids = {foster_social_2012},
  title = {Social Network Correlates of Food Availability in an Endangered Population of Killer Whales, {{Orcinus}} Orca},
  author = {Foster, Emma A. and Franks, Daniel W. and Morrell, Lesley J. and Balcomb, Ken C. and Parsons, Kim M. and van Ginneken, Astrid and Croft, Darren P.},
  date = {2012-03-01},
  journaltitle = {Animal Behaviour},
  shortjournal = {Animal Behaviour},
  volume = {83},
