@article{puseyMotheroffspringRelationshipsChimpanzees1983,
  ids = {pusey_mother-offspring_1983},
  title = {Mother-Offspring Relationships in Chimpanzees after Weaning},
  author = {Pusey, Anne E.},
  date = {1983-05-01},
  journaltitle = {Animal Behaviour},
  shortjournal = {Animal Behaviour},
  volume = {31},
