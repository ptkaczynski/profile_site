  file = {C\:\\Users\\Megaport\\Zotero\\storage\\RNF4Y7RW\\Gesquiere et al. - 2011 - Life at the Top Rank and Stress in Wild Male Babo.pdf;C\:\\Users\\Megaport\\Zotero\\storage\\2MSMM6EC\\357.html},
  langid = {english},
  number = {6040}
}

@article{gettlerBroadeningPerspectivesEvolution2020,
  title = {Broadening {{Perspectives}} on the {{Evolution}} of {{Human Paternal Care}} and {{Fathers}}’ {{Effects}} on {{Children}}},
  author = {Gettler, Lee T. and Boyette, Adam H. and Rosenbaum, Stacy},
  date = {2020},
  journaltitle = {Annual Review of Anthropology},
  volume = {49},
  pages = {141--160},
  doi = {10.1146/annurev-anthro-102218-011216},
  url = {https://doi.org/10.1146/annurev-anthro-102218-011216},
  urldate = {2020-12-11},
  abstract = {Unlike most mammals, human fathers cooperate with mothers to care for young to an extraordinary degree. Human paternal care likely evolved alongside our unique life history strategy of raising slow-developing, energetically costly children, often in rapid succession. Adaptive frameworks generally assume that paternal provisioning played a critical role in this pattern's emergence. We draw on nonhuman primate data to propose that nonprovisioning forms of low-cost hominin male care were potentially foundational and ratcheted up through evolutionary time, helping facilitate social contexts for later subsistence specialization and sharing. We then argue for expanding the breadth of anthropological research on paternal effects in families, particularly in three domains: direct care and teaching;social capital cultivation; and reduction of family conflict. Anthropologists can greatly contribute to conversations about the determinants of children's development across contexts, but we need to ask more expansive questions about the pathways through which caregivers (including fathers) affect child outcomes.},
  annotation = {\_eprint: https://doi.org/10.1146/annurev-anthro-102218-011216},
  file = {C\:\\Users\\Megaport\\Zotero\\storage\\BUUH37WI\\Gettler et al. - 2020 - Broadening Perspectives on the Evolution of Human .pdf},
  number = {1}
}

