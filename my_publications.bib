  ids = {denham_assessing_2009},
  title = {Assessing Social-Emotional Development in Children from a Longitudinal Perspective},
  author = {Denham, S. A. and Wyatt, T. M. and Bassett, H. H. and Echeverria, D. and Knox, S. S.},
