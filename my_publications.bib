@article{noeBirdsFeatherLocate2016,
  ids = {noe_birds_2016},
  title = {Birds of a Feather Locate Together? {{Foursquare}} Checkins and Personality Homophily},
  shorttitle = {Birds of a Feather Locate Together?},
  author = {Noë, Nyala and Whitaker, Roger M. and Chorley, Martin J. and Pollet, Thomas V.},
  date = {2016-05-01},
  journaltitle = {Computers in Human Behavior},
  shortjournal = {Computers in Human Behavior},
  volume = {58},
  pages = {343--353},
  issn = {0747-5632},
  doi = {10.1016/j.chb.2016.01.009},
  url = {http://www.sciencedirect.com/science/article/pii/S0747563216300097},
  urldate = {2020-02-15},
  abstract = {In this paper we consider whether people with similar personality traits have a preference for common locations. Due to the difficulty in tracking and categorising the places that individuals choose to visit, this is largely unexplored. However, the recent popularity of location-based social networks (LBSNs) provides a means to gain new insight into this question through checkins - records that are made by LBSN users of their presence at specific street level locations. A web-based participatory survey was used to collect the personality traits and checkin behaviour of 174 anonymous users, who, through their common check-ins, formed a network with 5373 edges and an approximate edge density of 35\%. We assess the degree of overlap in personality traits for users visiting common locations, as detected by user checkins. We find that people with similar high levels of conscientiousness, openness or agreeableness tended to have checked-in locations in common. The findings for extraverts were unexpected in that they did not provide evidence of individuals assorting at the same locations, contrary to predictions. Individuals high in neuroticism were in line with expectations, they did not tend to have locations in common. Unanticipated results concerning disagreeableness are of particular interest and suggest that different venue types and distinctive characteristics may act as attractors for people with particularly selective tendencies. These findings have important implications for decision-making and location.},
  file = {C\:\\Users\\Megaport\\Zotero\\storage\\VBLUETMV\\Noë et al. - 2016 - Birds of a feather locate together Foursquare che.pdf;C\:\\Users\\Megaport\\Zotero\\storage\\A996794W\\S0747563216300097.html},
  keywords = {Foursquare,Location-based social network,Personality,Spatial homophily},
  langid = {english}
}

@article{noePersonalityHomophilyGeographic2018,
  ids = {noe_personality_2018},
  title = {Personality {{Homophily}} and {{Geographic Distance}} in {{Facebook}}},
  author = {Noë, Nyala and Whitaker, Roger M. and Allen, Stuart M.},
  date = {2018-05-24},
  journaltitle = {Cyberpsychology, Behavior, and Social Networking},
  shortjournal = {Cyberpsychology, Behavior, and Social Networking},
  volume = {21},
  pages = {361--366},
