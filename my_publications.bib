@article{guindre-parkerIndividualVariationPhenotypic2019,
  ids = {guindre-parker_individual_2019},
  title = {Individual Variation in Phenotypic Plasticity of the Stress Axis},
  author = {Guindre-Parker, Sarah and Mcadam, Andrew G. and van Kesteren, Freya and Palme, Rupert and Boonstra, Rudy and Boutin, Stan and Lane, Jeffrey E. and Dantzer, Ben},
  date = {2019-07-26},
  journaltitle = {Biology Letters},
  shortjournal = {Biology Letters},
  volume = {15},
