}

@article{sapolskyGlucocorticoidsStressTheir1999,
  title = {Glucocorticoids, Stress, and Their Adverse Neurological Effects: Relevance to Aging},
  shorttitle = {Glucocorticoids, Stress, and Their Adverse Neurological Effects},
  author = {Sapolsky, Robert M},
  date = {1999-09-01},
  journaltitle = {Experimental Gerontology},
