@article{pittetMaternalStylesPrecocial2014,
  title = {Maternal Styles in a Precocial Bird},
  author = {Pittet, Florent and Houdelier, Cécilia and de Margerie, Emmanuel and Le Bot, Océane and Richard-Yris, Marie-Annick and Lumineau, Sophie},
